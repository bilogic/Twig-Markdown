{
    "name": "jralph/twig-markdown",
    "description": "A simple twig markdown extension.",
    "keywords": ["twig", "markdown", "parsedown"],
    "license": "MIT",
    "require": {
        "php": "~8.0",
        "twig/twig": "~2",
        "erusev/parsedown-extra": "0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Jralph\\Twig\\Markdown\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
